<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping  xmlns="urn:nhibernate-mapping-2.2">
  <class name="STTProjects.TimeSheet.Model.Worker, STTProjects.TimeSheet.Model" table="Workers" lazy = "true">

    <id name="ID" type="int" unsaved-value="null" >
      <column name="id" not-null="true"/>
      <generator class="identity"/>
    </id>

    <property column="Name" type="String" name="Name" length="300" />
    <property column="LastName" type="String" name="LastName" length="300" />

   

  </class>
</hibernate-mapping>
